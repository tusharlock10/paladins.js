    "jest": "^28.1.0",
    "mocha": "^10.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.8.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
